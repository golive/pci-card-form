  "homepage": "https://github.com/sequra/mufasa-js-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "esbuild": "^0.14.23",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.18.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
